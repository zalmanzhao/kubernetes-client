{
	"name": "zalmanzhao/kubernetes-client",
	"description": "A simple yet elegant client for accessing and controlling a Kubernetes cluster.",
	"keywords": ["kubernetes", "docker", "container", "cluster", "guzzle", "guzzlehttp"],
	"authors": [
		{
			"name": "Marc Lough",
			"homepage": "http://zalmanzhao.com"
		}
	],
	"license" : "MIT",
	"require": {
		"php": ">=5.4",
		"guzzlehttp/guzzle": "~5.0|~6.0",
		"illuminate/support": "~4.0|~5.0"
	},
	"require-dev": {
		"phpunit/phpunit" : "4.8.36",
		"mockery/mockery" : "0.9.9",
		"satooshi/php-coveralls": "1.0.1"
	},
	"autoload": {
		"psr-4": {
			"Maclof\\Kubernetes\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
